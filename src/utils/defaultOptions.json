{
  "authentication": {
    "emails": {
      "required": false
    },
    "passwords": {
      "saltRounds": 1,
      "tries": 5
    },
    "tokens": {
      "length": 15,
      "tries": 5
    }
  },
  "storageSize": {
    "owner": "Infinity",
    "admin": "Infinity",
    "staff": "1e+7",
    "friend": "5e+6",
    "tier-3": "2e+7",
    "tier-2": "1e+7",
    "tier-1": "5e+6",
    "user": "2e+6",
    "bad": "1e+6"
  },
  "mongodb": {
    "connectURI": "mongodb: //localhost/sharex-rewrite-test",
    "connectOptions": {
      "useNewUrlParser": true,
      "useUnifiedTopology": true
    }
  },
  "useRedis": false,
  "defaults": {
    "port": 80,
    "domain": "localhost",
    "secure": false,
    "userType": "user"
  }
}